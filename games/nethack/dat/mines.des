FLAGS:mazelevel,noflip
INIT_MAP: mines, '.' , ' ' , true , true , random , true
NOMAP
#
STAIR: random, up
STAIR: random, down
#
OBJECT: '*', random
OBJECT: '*', random
OBJECT: '*', random
OBJECT: '(', random
LOOP [2d2] {
  OBJECT: random, random
}
#
LOOP [4 + 2d5] {
  MONSTER: ('G', "gnome"), random
}
MONSTER: ('G', "gnome lord"), random
MONSTER: ('h', "dwarf"), random
MONSTER: ('h', "dwarf"), random
MONSTER: 'G', random
MONSTER: 'G', random
MONSTER: 'h', random
#
LOOP [4 + 1d6] {
  TRAP: random, random
}

